{% set domainPath = symkaLayout(false) %}
<!DOCTYPE html>
<html lang="en">

<head>

    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="">
    <meta name="author" content="">

    <title>{% block title %}{% endblock %}</title>
    <link rel="shortcut icon" href="{{ asset(domainPath~'/img/favicon.ico') }}">

    <!-- Global Stylesheets -->
    <link href="https://fonts.googleapis.com/css?family=Roboto+Condensed:300,300i,400,400i,700,700i" rel="stylesheet">
    <link href="{{ asset(domainPath~'/css/bootstrap/bootstrap.min.css') }}" rel="stylesheet">
    <link rel="stylesheet" href="{{ asset(domainPath~'/font-awesome-4.7.0/css/font-awesome.min.css') }}">
    <link rel="stylesheet" href="{{ asset(domainPath~'/css/animate/animate.min.css') }}">
    <link rel="stylesheet" href="{{ asset(domainPath~'/css/owl-carousel/owl.carousel.min.css') }}">
    <link rel="stylesheet" href="{{ asset(domainPath~'/css/owl-carousel/owl.theme.default.min.css') }}">
    <link rel="stylesheet" href="{{ asset(domainPath~'/css/style.css') }}">
</head>


{% block body %}
    <body>
    {% block header %}{% endblock %}

    {% block content %}{% endblock %}

    {% block footer %}{% endblock %}

    {% block javascripts %}
        <script src="{{ asset(domainPath~'/js/jquery/jquery.min.js') }}"></script>
        <script src="{{ asset(domainPath~'/js/popper/popper.min.js') }}"></script>
        <script src="{{ asset(domainPath~'/js/bootstrap/bootstrap.min.js') }}"></script>
        <script src="{{ asset(domainPath~'/js/wow/wow.min.js') }}"></script>
        <script src="{{ asset(domainPath~'/js/owl-carousel/owl.carousel.min.js') }}"></script>

        <!-- Plugin JavaScript -->
        <script src="{{ asset(domainPath~'/js/jquery-easing/jquery.easing.min.js') }}"></script>
        <script src="{{ asset(domainPath~'/js/custom.js') }}"></script>
    {% endblock %}

    </body>

{% endblock %}


</html>
